{
	"presets": [
		"@babel/preset-env",
		[
			"@babel/preset-react",
			{
				"runtime": "automatic"
			}
		],
		"@babel/preset-typescript"
	],
	"plugins": [
		[
			"@babel/plugin-transform-runtime",
			{
				"regenerator": true
			}
		],
		[
			"module-resolver",
			{
				"root": ["."],
				"alias": {
					"@": "./src/",
					"@components": "./src/components/",
					"@interfaces": "./src/interfaces/",
					"@styles": "./src/styles/",
					"@layouts": "./src/layouts/"
				}
			}
		]
	]
}
